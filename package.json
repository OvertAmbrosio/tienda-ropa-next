{
  "name": "tienda-ropa-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000",
    "build": "next build",
    "start": "next start -p 3000",
    "lint": "next lint",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init",
    "prisma:seed": "node prisma/seed.mjs",
    "setup": "node scripts/prepare.mjs && npx prisma generate && npx prisma migrate dev --name init && npm run prisma:seed",
    "start:prod": "node scripts/prepare.mjs && npx prisma generate && npx prisma migrate deploy && next build && next start -p 3000"
  },
  "dependencies": {
    "@prisma/client": "5.19.1",
    "bcryptjs": "2.4.3",
    "next": "14.2.5",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "24.6.2",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "autoprefixer": "10.4.19",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.5",
    "postcss": "8.4.41",
    "prisma": "5.19.1",
    "tailwindcss": "3.4.10",
    "typescript": "5.5.4"
  }
}
